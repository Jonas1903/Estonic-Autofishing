{
  "schemaVersion": 1,
  "id": "estonic-autofishing",
  "version": "${version}",
  "name": "Estonic Autofishing",
  "description": "Automatic fishing mod that detects fishing cues and automates rod actions with randomized movement to avoid anti-cheat detection.",
  "authors": [
    "Estonic"
  ],
  "contact": {
    "homepage": "https://github.com/Jonas1903/Estonic-Autofishing",
    "sources": "https://github.com/Jonas1903/Estonic-Autofishing"
  },
  "license": "MIT",
  "icon": "assets/estonic-autofishing/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.estonic.autofishing.AutofishingMod"
    ]
  },
  "mixins": [
    "estonic-autofishing.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.0",
    "fabric-api": "*",
    "minecraft": "~1.21",
    "java": ">=21"
  },
  "suggests": {
    "another-mod": "*"
  }
}
